import rodados.*

describe "probando un Corsa" {
  const miCorsa = new Corsa(color = "rojo")
  test "su capacidad"{
    assert.equals(4, miCorsa.capacidad())
  }
  test "su velocidad maxima"{
    assert.equals(150, miCorsa.velocidadMaxima())
  }
  test "su peso"{
    assert.equals(1300, miCorsa.peso())
  } 

}